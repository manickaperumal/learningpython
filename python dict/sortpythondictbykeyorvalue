pgm:

		def dictionary():
		    keyvalue={}
		    keyvalue[2]=56
		    keyvalue[1]=2
		    keyvalue[5]=12
		    keyvalue[4]=24
		    keyvalue[6]=18
		    keyvalue[3]=323
		    print(keyvalue)

		    for i in sorted(keyvalue.keys()):
			print(i,end=" ")

		dictionary()
		
output:

		{2: 56, 1: 2, 5: 12, 4: 24, 6: 18, 3: 323}
		
		1 2 3 4 5 6 
		
		
pgm:

		d={"name":"manic","age":21,"degree":"Bsc","sports":None}
		for i in sorted(d.keys()):
		    print(i,end=" ")


